{
  "name": "soft",
  "version": "0.0.0",
  "description": "html-esque templating engine",
  "main": "build/soft.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "tap test/*.js",
    
    "build": "rollup -f umd -n soft src/soft.js > build/soft.js",
    
    "prewatch": "npm run build",
    "watch": "wr 'npm run build' src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SundarJ/soft.git"
  },
  "keywords": [
    "templating",
    "engine",
    "template",
    "soft",
    "html",
    "render"
  ],
  "author": "Sundar Joshi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SundarJ/soft/issues"
  },
  "homepage": "https://github.com/SundarJ/soft#readme",
  "devDependencies": {
    "rollup": "~0.25.0",
    "tap": "~5.1.1",
    "wr": "^1.3.1"
  },
  "dependencies": {}
}
