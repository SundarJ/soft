{
    "extends": "airbnb",
    
    "rules": {
      "semi": [2, "never"],
      "no-trailing-spaces": [2, { "skipBlankLines": true }],
      "space-in-parens": 0,
      "no-cond-assign": [2, "except-parens"],
      "no-param-reassign": 0,
      "eqeqeq": [2, "smart"],
      "no-new-func": 0,
      "padded-blocks": 0,
      "arrow-parens": [2, "as-needed"],
      "arrow-body-style": [2, "as-needed"]
    }
}